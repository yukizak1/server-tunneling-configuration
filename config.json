{
	"log": {
		"access": "/var/log/Xray/access.log",
		"error": "/var/log/xray/error.log",
		"loglevel": "none",
		"dnsLog": false
	},
	"inbounds": [{
		"listen": "127.0.0.1",
		"port": 80,
		"protocol": "vless",
		"settings": {
			"clients": [{
				"id": "29db4508-0e41-436b-b36f-5d7b1e901555",
				"level": 0
			}],
			"decryption": "none"
		},
		"streamSettings": {
			"network": "ws",
			"wsSettings": {
				"path": "/akagi",
				"headers": {
					"Host": ""
				}
			},
			"sockopt": {
					"tcpFastOpen": true,
					"tproxy": "tproxy"
				},
			"sniffing": {
				"enabled": true,
				"destOverride": ["http", "tls"]
				},
			"allocate": {
				"strategy": "always",
				"refresh": 5,
				"concurrency": 3
			}
		}
	}],
	"outbounds": [{
			"protocol": "freedom",
			"settings": {},
			"mux": {
				"enabled": true,
				"concurrency": 1024
			}
		}],
	"stats": {},
	"api": {
		"services": [
			"StatsService"
		],
		"tag": "api"
	},
	"policy": {
		"levels": {
			"0": {
				"statsUserDownlink": true,
				"statsUserUplink": true
			}
		},
		"system": {
			"statsInboundUplink": true,
			"statsInboundDownlink": true
		}
	}
}